<Window x:Class="Sokoban_Game_WPF.FinishScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sokoban_Game_WPF"
        mc:Ignorable="d"
        Title="FinishScreen" Height="450" Width="433">
    <Grid>
        <Label Content="You Win" HorizontalAlignment="Center" Margin="0,29,0,0" VerticalAlignment="Top" FontFamily="Unispace" FontSize="28"/>
        <Label Content="Moves:" HorizontalAlignment="Left" Margin="173,78,0,0" VerticalAlignment="Top" FontFamily="Unispace" FontSize="16"/>
        <Label x:Name="MoveCount" Content="10" HorizontalAlignment="Left" Margin="235,78,0,0" VerticalAlignment="Top" FontFamily="Unispace" FontSize="16"/>
        <Button Content="Restart" HorizontalAlignment="Center" Margin="0,227,0,0" VerticalAlignment="Top" FontFamily="Tahoma" FontSize="20" Click="Button_Click"/>
        <Button Content="Quit" HorizontalAlignment="Center" Margin="0,289,0,0" VerticalAlignment="Top" FontFamily="Tahoma" FontSize="20" Width="67" Click="Button_Click_1"/>

    </Grid>
</Window>
